{
  "name": "firebase-database-modeler",
  "version": "2.8.0",
  "description": "Create and use Firebase Realtime Database models",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run deploy",
    "build": "rm -rf lib && tsc -d",
    "deploy": "npm run build && npm publish",
    "major": "npm --no-git-tag-version version major",
    "majorD": "npm run build && npm run major && npm publish",
    "minor": "npm --no-git-tag-version version minor",
    "minorD": "npm run build && npm run minor && npm publish",
    "patch": "npm --no-git-tag-version version patch",
    "patchD": "npm run build && npm run patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SrBrahma/Firebase-Database-Modeler.git"
  },
  "keywords": [
    "firebase",
    "google",
    "realtime",
    "database",
    "model"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SrBrahma/Firebase-Database-Modeler/issues"
  },
  "homepage": "https://github.com/SrBrahma/Firebase-Database-Modeler#readme",
  "dependencies": {
    "@firebase/database-types": "^0.5.2",
    "@firebase/logger": "^0.2.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "typescript": "^4.0.2"
  }
}
